{
	"manifest_version": 2,
	"name": "Bivariate-test-helper",
	"description": "generate user interactions",
	"author": "Justin Parker",
	"version": "1.0.0",
	"permissions": [
		"activeTab"
	],
	"content_security_policy": "script-src 'self'; object-src 'self'",
	"background":{
		"persistent": false,
		"scripts": [ "background.js" ]
	},
	"browser_action": {
		"default_icon": "icon.png",
		"default_title": "Click to enable/disable Bivariate test helper..."
	}
}