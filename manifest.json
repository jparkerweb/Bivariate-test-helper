{
	"manifest_version": 2,
	"name": "Bivariate-test-helper",
	"description": "generate user interactions",
	"version": "1.0",
	"permissions": [
		"tabs",
		"https://*/*",
		"http://*/*",
		"activeTab"
	],
	"content_security_policy": "script-src 'self'; object-src 'self'",
	"background":{
		"persistent": false,
		"scripts": [ "background.js" ]
	},
	"browser_action": {
		"default_icon": "icon.png"
	}
}